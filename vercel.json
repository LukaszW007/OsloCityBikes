{
  "version": 2,
  "builds": [
      {
				"src": "public/client/build/index.js",
				"use": "@now/node"
      },
      {
				"src": "public/server/build/index.js",
				"use": "@now/static"
      }
  ],
  "routes": [
      {
				"src": "/public/(.*)",
				"dest": "public/server/build/index.js"
      },
      { 
        "src": "/public/(.*)",
        "dest": "public/client/build/index.js"
      }
  ]
}